<<Create_miRNA_Batch_Effect, echo = FALSE, message = FALSE>>=
raw.mi <- data.matrix(raw.w.mi[-c(1:5), which(names(raw.w.mi) %in% annodf.mi$SampleID)])

#create batch matrix
bmat <- data.frame(row.names = rownames(raw.mi))

for(i in 1:ncol(raw.mi)){
  if(grepl("228", colnames(raw.mi)[i])){
    #colvec <- rpois(nrow(raw.mi), lambda = .5*raw.mi[, i]) * runif(nrow(raw.mi), c(-1,1))
    colvec <- rpois(nrow(raw.mi), lambda = raw.mi[, i])
  }else{
    colvec <- rep(0, nrow(raw.mi))
  }
  bmat <- cbind(bmat, colvec)
}
names(bmat) <- colnames(raw.mi)

bedf <- raw.mi + bmat
#truncate at 0
bedf <- apply(bedf, c(1, 2), function(x) if(x<0) x <- 0 else x <- x)

#log transform
bedfl <- apply(bedf, c(1,2), function(x) log2(x + 1))

pcplotdatl.bedf <- plotPCA(bedfl, annodf.mi, scale = FALSE)
ggpcal.mi <- ggplot(pcplotdatl.bedf[[1]], aes(x= PC1, y= PC2, color=Plate, shape=SampType)) + 
    geom_point( size = 3)  + 
  annotate("text", label = paste0("Cum. proportion of var. = ", round(pcplotdatl.bedf[[2]], 2)), x = 0, y = 0, color = "black") + 
  coord_equal() + ylim(c(-250, 100)) +
  ggtitle("log(Raw) Transformed")
# ggsave("./Paper1/Figures/miRNA_Repro_PCA_log(raw).png", ggpcal.mi, width = 9, height = 8 )
ggplot(pcplotdatl.bedf[[1]], aes(x= PC1, y= PC3, color=Plate, shape=SampType)) + geom_point( size = 3)
ggplot(pcplotdatl.bedf[[1]], aes(x= PC2, y= PC3, color=Plate, shape=SampType)) + geom_point( size = 3)
ggplot(pcplotdatl.bedf[[1]], aes(x= PC2, y= PC4, color=Plate, shape=SampType)) + geom_point( size = 3)


